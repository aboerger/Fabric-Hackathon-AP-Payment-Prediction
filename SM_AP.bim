        "partitions": [
          {
            "name": "_measures",
            "mode": "directLake",
            "source": {
              "type": "entity",
              "entityName": "_measures",
              "expressionSource": "DatabaseQuery",
              "schemaName": "dbo"
            }
          }
        ],
        "measures": [
          {
            "name": "Before Due Balance (GC)",
            "description": "This measure calculates the sum of the 'amount_group_currency' field in the 'ap_line_items' table for all records where the 'clearing_date' is either blank or later than the maximum date in the 'calendar' table, and the 'net_due_date' is on or after the maximum date in the 'calendar' table. The calculation is performed up to the maximum date in the 'calendar' table.",
            "expression": [
              "",
              "    CALCULATE(",
              "        SUM(ap_line_items_scored[amount_group_currency]),",
              "        OR(",
              "            ISBLANK(ap_line_items_scored[clearing_date]),",
              "            ap_line_items_scored[clearing_date] > MAX('calendar'[date])",
              "        ),",
              "        ap_line_items_scored[net_due_date] >= MAX('calendar'[date]),",
              "        FILTER(",
              "            ALL('calendar'[date]),",
              "            'calendar'[date] <= MAX('calendar'[date])",
              "        )",
              "    )"
            ],
            "lineageTag": "747af4d9-baad-459b-bee7-0a8729faec93",
            "changedProperties": [
              {
                "property": "Name"
              },
              {
                "property": "Description"
              },
              {
                "property": "DisplayFolder"
              }
            ],
            "annotations": [
              {
                "name": "PBI_FormatHint",
                "value": "{\"isGeneralNumber\":true}"
              }
            ]
          },
          {
            "name": "Overdue Balance (GC)",
            "description": "The 'Overdue Balance (GC)' measure calculates the sum of the 'amount_group_currency' field from the 'ap_line_items' table for all records where the 'clearing_date' is either blank or greater than the maximum date in the 'calendar' table, and the 'net_due_date' is equal to or greater than the maximum date in the 'calendar' table. The calculation is performed up to the maximum date in the 'calendar' table.",
            "expression": [
              "",
              "    CALCULATE(",
              "        SUM(ap_line_items_scored[amount_group_currency]),",
              "        OR(",
              "            ISBLANK(ap_line_items_scored[clearing_date]),",
              "            ap_line_items_scored[clearing_date] > MAX('calendar'[date])",
              "        ),",
              "        ap_line_items_scored[net_due_date] >= MAX('calendar'[date]),",
              "        FILTER(",
              "            ALL('calendar'[date]),",
              "            'calendar'[date] <= MAX('calendar'[date])",
              "        )",
              "    )"
            ],
            "lineageTag": "6de2bb40-df1e-4966-8131-fee324444082",
            "changedProperties": [
              {
                "property": "Name"
              },
              {
                "property": "Description"
              }
            ],
            "annotations": [
              {
                "name": "PBI_FormatHint",
                "value": "{\"isGeneralNumber\":true}"
              }
            ]
          },
          {
            "name": "Open Balance (GC)",
            "description": "The 'Open Balance (GC)' measure calculates the sum of the 'amount_group_currency' field from the 'ap_line_items' table. It includes only those records where the 'clearing_date' is either blank or greater than the maximum date in the 'calendar' table, and where the 'calendar' date is less than or equal to the maximum 'calendar' date.",
            "expression": [
              "",
              "    CALCULATE(",
              "        SUM(ap_line_items_scored[amount_group_currency]),",
              "        OR(",
              "            ISBLANK(ap_line_items_scored[clearing_date]),",
              "            ap_line_items_scored[clearing_date] > MAX('calendar'[date])",
              "        ),",
              "        FILTER(",
              "            ALL('calendar'[date]),",
              "            'calendar'[date] <= MAX('calendar'[date])",
              "        )",
              "    )"
            ],
            "lineageTag": "79a5ce40-8f1d-442e-b7ae-2a2ffda714af",
            "changedProperties": [
              {
                "property": "Name"
              },
              {
                "property": "Description"
              }
            ],
            "annotations": [
              {
                "name": "PBI_FormatHint",
                "value": "{\"isGeneralNumber\":true}"
              }
            ]
          },
          {
            "name": "No. of Open Invoices",
            "description": "This measure calculates the distinct count of accounting documents in the 'ap_line_items' table. It only includes documents of specific types and those that are either not cleared or have a clearing date later than the maximum date in the 'calendar' table.",
            "expression": [
              "",
              "CALCULATE(",
              "    DISTINCTCOUNT(ap_line_items_scored[accounting_document]),",
              "        OR(",
              "            document_types[document_type] = \"RE\", ",
              "            document_types[document_type] = \"KR\"",
              "            ),",
              "    OR(",
              "        ISBLANK(ap_line_items_scored[clearing_date] ),",
              "        ap_line_items_scored[clearing_date] > MAX('calendar'[date])",
              "    ),",
              "    FILTER(",
              "        ALL('calendar'[date]),",
              "        'calendar'[date] <= MAX('calendar'[date])",
              "    )",
              ")"
            ],
            "formatString": "0",
            "lineageTag": "07bc3447-40ab-4d2f-8176-749258ef3415",
            "changedProperties": [
              {
                "property": "Name"
              },
              {
                "property": "Description"
              }
            ]
          },
          {
            "name": "No. of Invoices Cleared",
            "description": "This measure calculates the distinct count of accounting documents in the 'ap_line_items' table where the document type matches one of two specified string literals. It uses a relationship between the 'calendar' table's date and the 'ap_line_items' table's clearing date.",
            "expression": [
              "",
              "CALCULATE(",
              "    DISTINCTCOUNT(ap_line_items_scored[accounting_document]),",
              "    OR(",
              "        ap_line_items_scored[document_type] = \"RE\", ",
              "        ap_line_items_scored[document_type]  = \"KR\"",
              "    ),",
              "    USERELATIONSHIP('calendar'[date], ap_line_items_scored[clearing_date])",
              ")"
            ],
            "formatString": "0",
            "lineageTag": "652c2b54-f766-4fb4-8341-ce3bff559c5f",
            "changedProperties": [
              {
                "property": "Name"
              },
              {
                "property": "Description"
              }
            ]
          },
          {
            "name": "Avg. Invoice Cleared in Days",
            "description": "This measure calculates the average number of days it takes to clear an invoice. It does this by subtracting the baseline date from the clearing date for each line item in the 'ap_line_items' table. The measure only considers line items of certain document types and those that have a non-blank clearing date or a clearing date that is later than the maximum date in the 'calendar' table.",
            "expression": [
              "",
              "CALCULATE(",
              "    AVERAGEX(",
              "        ap_line_items_scored,",
              "        ap_line_items_scored[clearing_date] - ap_line_items_scored[baseline_date]",
              "    ),",
              "        OR(",
              "        ap_line_items_scored[document_type] = \"RE\", ",
              "        ap_line_items_scored[document_type] = \"KR\"",
              "    ),",
              "    OR(",
              "        NOT ISBLANK(ap_line_items_scored[clearing_date]),",
              "        ap_line_items_scored[clearing_date] > MAX('calendar'[date])",
              "    )",
              ")"
            ],
            "formatString": "0.0",
            "lineageTag": "4400e948-da41-41a4-8576-134ec5419d88",
            "changedProperties": [
              {
                "property": "Name"
              },
              {
                "property": "Description"
              },
              {
                "property": "FormatString"
              }
            ]
          },
          {
            "name": "Invoices Cleared Before Due %",
            "description": "This measure calculates the percentage of invoices that were cleared before their due date. It does this by dividing the number of distinct accounting documents that were cleared before their due date by the total number of distinct accounting documents that have a clearing date.",
            "expression": [
              "",
              "DIVIDE(",
              "    CALCULATE(",
              "        DISTINCTCOUNT(ap_line_items_scored[accounting_document]),",
              "        OR(",
              "            ap_line_items_scored[document_type] = \"RE\", ",
              "            ap_line_items_scored[document_type] = \"KR\"",
              "        ),",
              "        OR(",
              "            NOT ISBLANK(ap_line_items_scored[clearing_date]),",
              "            ap_line_items_scored[clearing_date] > MAX('calendar'[date])",
              "        ),",
              "        ap_line_items_scored[clearing_date] <= ap_line_items_scored[net_due_date]",
              "    )",
              "    ,",
              "    CALCULATE(",
              "        DISTINCTCOUNT(ap_line_items_scored[accounting_document]),",
              "        OR(",
              "            ap_line_items_scored[document_type] = \"RE\", ",
              "            ap_line_items_scored[document_type] = \"KR\"",
              "        ),",
              "        OR(",
              "            NOT ISBLANK(ap_line_items_scored[clearing_date]),",
              "            ap_line_items_scored[clearing_date] > MAX('calendar'[date])",
              "        )",
              "    ),",
              "    1",
              ")"
            ],
            "formatString": "0.0%;-0.0%;0.0%",
            "lineageTag": "aa202c10-784c-476d-9f2e-c657b74d6fbd",
            "changedProperties": [
              {
                "property": "Name"
              },
              {
                "property": "Description"
              },
              {
                "property": "FormatString"
              }
            ]
          },
          {
            "name": "Avg. Invoice Overdue Days",
            "description": "This measure calculates the average number of days that invoices are overdue. It does this by subtracting the net due date from the current date for each line item in the 'ap_line_items' table, but only for those line items where the clearing date is either blank or later than the current date, and where the current date is later than the net due date.",
            "expression": [
              "",
              "CALCULATE(",
              "    AVERAGEX(",
              "        ap_line_items_scored,",
              "        MAX('calendar'[date]) - ap_line_items_scored[net_due_date]",
              "    ),",
              "    OR(",
              "        ISBLANK(ap_line_items_scored[clearing_date]),",
              "        ap_line_items_scored[clearing_date] > MAX('calendar'[date])",
              "    ),",
              "    MAX('calendar'[date]) > ap_line_items_scored[net_due_date]",
              ")"
            ],
            "formatString": "0.0",
            "lineageTag": "dd5be08e-e38d-4620-b88d-416963255ea6",
            "changedProperties": [
              {
                "property": "Name"
              },
              {
                "property": "Description"
              },
              {
                "property": "FormatString"
              }
            ]
          },
          {
            "name": "Overdue Balance %",
            "description": "The 'Overdue Balance %' measure calculates the percentage of the overdue balance to the open balance. It uses the DIVIDE function to divide the 'Overdue Balance (GC)' by the 'Open Balance (GC)', returning 1 if the 'Open Balance (GC)' is zero.",
            "expression": "DIVIDE([Overdue Balance (GC)],[Open Balance (GC)],1)",
            "formatString": "0.0%;-0.0%;0.0%",
            "lineageTag": "ad08bc94-0dcf-4e70-bd04-ab1948552c83",
            "changedProperties": [
              {
                "property": "Name"
              },
              {
                "property": "Description"
              },
              {
                "property": "FormatString"
              }
            ]
          },
          {
            "name": "Avg. Payment Terms",
            "description": "This measure calculates the average payment terms in days from the baseline date for payment. It does this for each line item in the 'ap_line_items' table where the document type is either of two specific string literals.",
            "expression": [
              "",
              "CALCULATE(",
              "    AVERAGEX(",
              "        ap_line_items_scored,",
              "        RELATED(payment_terms[days_from_baseline_date_for_payment])",
              "    ),",
              "    OR(",
              "        ap_line_items_scored[document_type] = \"RE\", ",
              "        ap_line_items_scored[document_type] = \"KR\"",
              "    )",
              ")"
