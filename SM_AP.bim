          {
            "name": "No. of Overdue Invoices Cleared",
            "description": "This measure calculates the distinct count of accounting documents in the 'ap_line_items' table where the document type is either of two specified string literals, and the clearing date is later than the net due date. It uses a relationship between the 'calendar' date and the 'ap_line_items' clearing date.",
            "expression": [
              "",
              "CALCULATE(",
              "    DISTINCTCOUNT(ap_line_items_scored[accounting_document]),",
              "    OR(",
              "        document_types[document_type] = \"DR\", ",
              "        document_types[document_type] = \"RV\"",
              "    ),",
              "    USERELATIONSHIP('calendar'[date], ap_line_items_scored[clearing_date]),",
              "    ap_line_items_scored[clearing_date] > ap_line_items_scored[net_due_date]",
