          {
            "name": "Overdue 1-30 Days",
            "description": "This measure, 'Overdue 1-30 Days', calculates the sum of the 'amount_group_currency' field from the 'ap_line_items' table for all records where the 'net_due_date' is less than the maximum date in the 'calendar' table and the 'clearing_date' is either blank or greater than the maximum date in the 'calendar' table. The measure only considers records where the difference between the 'net_due_date' and the maximum date in the 'calendar' table is between 1 and 30 days.",
            "expression": [
              "",
              "VAR ",
              "    _KeyDate = MAX('calendar'[date])",
              "RETURN",
              "    CALCULATE(",
              "        SUM(ap_line_items_scored[amount_group_currency]),",
              "        OR(",
              "            ISBLANK(ap_line_items_scored[clearing_date]),",
              "            ap_line_items_scored[clearing_date] > MAX('calendar'[date])",
              "        ),",
              "        ap_line_items_scored[net_due_date] < MAX('calendar'[date]),",
              "        FILTER(",
              "            ALL('calendar'[date]),",
              "            'calendar'[date] <= MAX('calendar'[date])",
              "        ),",
              "        DATEDIFF(ap_line_items_scored[net_due_date], _KeyDate, DAY) > 0,",
              "        DATEDIFF(ap_line_items_scored[net_due_date], _KeyDate, DAY) <= 30",
              "    )"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Aging Buckets",
            "lineageTag": "ee595bb1-4d7b-4f3d-aae7-24fa8df2e48b",
